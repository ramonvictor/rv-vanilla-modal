{
  "camelcase": false,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "immed": true,
  "indent": 3,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "plusplus": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "maxparams": 3,
  "maxlen": 80,
  "asi": true,
  "laxcomma": true,
  "white": true,
  "predef": [ "document", "console", "window" ]
}
